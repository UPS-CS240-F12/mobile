<?xml version="1.0"?>
<QCARConfig xsi:noNamespaceSchemaLocation="qcar_config.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <Tracking>
        <ImageTarget name="FlakesBox.Front" size="90 120"/>
        <ImageTarget name="FlakesBox.Top" size="90 60"/>
        <ImageTarget name="FlakesBox.Bottom" size="90 60"/>
        <ImageTarget name="FlakesBox.Left" size="60 120"/>
        <ImageTarget name="FlakesBox.Right" size="60 120"/>
        <ImageTarget name="FlakesBox.Back" size="90 120"/>
      <MultiTarget name="FlakesBox">
      <!-- translation='x, y, z' rotaion='x, y, z, deg' -->
      <Part name="FlakesBox.Front" translation="0 115 0" rotation="AD: 1 0 0 0"/>
      <Part name="FlakesBox.Back" translation="0 -115 0" rotation="AD: 1 0 0 0"/>
      <Part name="FlakesBox.Left" translation="-195 0 0" rotation="AD: 1 0 0 0"/>
      <Part name="FlakesBox.Right" translation="195 0 0" rotation="AD: 1 0 0 0"/>
      <Part name="FlakesBox.Top" translation="0 255 0" rotation="AD: 1 0 0 0"/>
      <Part name="FlakesBox.Bottom" translation="0 -255 0" rotation="AD: 1 0 0 0"/>
    </MultiTarget>
  </Tracking>
</QCARConfig>
